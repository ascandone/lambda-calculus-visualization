Expr {
  MAIN
    = Appl

  Appl
    = Appl Exp -- appl
    | Exp

  Exp
    = "(" Appl ")"  -- paren
    | ("\\" | "Î»") ident+ "." Appl -- abs
    | ident

  ident  (an identifier)
    = identHead identBody*

  identHead
    = letter
    | "_"
  
  identBody
    = alnum
    | "_"
  

  number  (a number)
    = digit+

  newline = "\n" | "\r"
  comment = "//" (~newline any)* newline

  space += comment
}
